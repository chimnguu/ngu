local KillAura = main:Toggle('Kill Aura',function(v) getgenv().BreakOut = v end)
getgenv().customui = true
loadstring(game:HttpGet("https://raw.githubusercontent.com/HazeWasTaken/JailedHax/master/AutRob"))()
